{
  "name": "backend",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npx prisma studio --browser none & nodemon -r dotenv/config src/index.ts",
    "build": "tsc",
    "start": "ts-node -r dotenv/config src/index.ts",
    "test": "prisma db seed && jest -i --forceExit",
    "setup": "prisma generate && prisma migrate dev && prisma db seed && npm run swagger",
    "setup:ci": "prisma generate && prisma migrate deploy && prisma db seed && npm run swagger",
    "swagger": "ts-node -r dotenv/config src/utils/swagger.ts",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prisma": "^5.20.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.2.2"
}
